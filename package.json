{
  "name": "code-font-gallery",
  "private": true,
  "version": "0.2.3",
  "type": "module",
  "scripts": {
    "dev_serve": "vite --host 0.0.0.0",
    "dist": "vite build",
    "build": "run-s fonts dist",
    "lint": "eslint src --ext js,jsx --report-unused-disable-directives --max-warnings 0 && echo 'Linting ok'",
    "watch_lint": "chokidar src -c 'run-s lint'",
    "preview": "vite preview",
    "dev": "run-p -c dev_serve watch_lint",
    "fonts": "./get_fonts",
    "test": "vitest"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.11.16",
    "@mui/material": "^5.14.0",
    "core-js": "^3.31.1",
    "lodash-es": "^4.17.21",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-syntax-highlighter": "^15.5.0",
    "use-local-storage-state": "^18.3.3"
  },
  "devDependencies": {
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "@vitejs/plugin-react": "^4.0.1",
    "chokidar-cli": "^3.0.0",
    "eslint": "^8.44.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.1",
    "npm-run-all": "^4.1.5",
    "vite": "^4.4.0",
    "vitest": "^0.32.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
